{
    "$schema": "https://swc.rs/schema.json",
    "module": {
        "type": "commonjs"
    },
    "sourceMaps": true,
    "jsc": {
        "target": "es2022",
        "baseUrl": "./src",
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": false,
            "privateMethod": false,
            "functionBind": false,
            "exportDefaultFrom": false,
            "exportNamespaceFrom": false,
            "decoratorsBeforeExport": false,
            "topLevelAwait": false,
            "importMeta": false
        }
    }
}
  